{
	"schemaVersion": 1,
	"id": "easyentityride",
	"version": "${version}",
	"name": "Easy Entity Ride",
	"description": "A client-side mod that allows easy mounting of entities in Creative mode.",
	"authors": [
		"Me"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/easyentityride/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.example.easyentityride.EasyEntityRideClient"
		]
	},
	"mixins": [
		"easyentityride.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}
